@using BingHousing_BO
@{
    ViewBag.Title = "Active Users - Swift Bill Pay";
    ViewBag.PageTitle = "Active Users";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}
@if (ViewBag.Results != null && ((List<AccountUserModel>)ViewBag.Results).Count > 0)
{
    <div class="panel-body">
        <div class="dataTable_wrapper">
            <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                <thead>
                    <tr>
                        <th>User Name</th>
                        <th>Operation</th>
                    </tr>
                </thead>
                <tbody>

                   @foreach (AccountUserModel L in (List<AccountUserModel>)ViewBag.Results)
                   {
                        <tr class="odd gradeX">
                            <td>@L.Name</td>
                            <td>
                                <a href="@Url.Action("LockUser", "User", new { UserId = L.UserId })"><i class="fa fa-archive"></i> Archive</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>

    </div>
}
else
{
    <h5>No active users found!</h5>
}
